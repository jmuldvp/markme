<%= content_tag :tr, class: "media", id: "bookmark-#{bookmark.url}" do %>
  <!-- <td><%#= link_to "#{bookmark.url}", "#{bookmark.url}" %></td> -->
  <td><%= link_to bookmark.url, bookmark.url, :target => '_blank' %></td>
  <% if current_user %>
    <!-- <td><%#= link_to "Edit", edit_topic_bookmark_path(@topic, @bookmark) %></td> -->
    <!-- <td><%#= link_to "Edit", edit_topic_bookmark_path([@topic, @bookmark]) %></td> -->
    <td><%= link_to "Edit", edit_bookmark_path([@topic, @bookmark]) %></td>
    <td><%= link_to "Delete", [bookmark.topic, bookmark], method: :delete %></td>
    <td><%= render partial: 'likes/like', locals: { bookmark: bookmark } %></td>
  <% end %>
<% end %>
